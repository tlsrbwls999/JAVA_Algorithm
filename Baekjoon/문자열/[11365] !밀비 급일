import java.util.Scanner;

public class Main {

    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        
        while(true){
            String str=sc.nextLine();
            if(str.equals("END")){
                System.exit(0);
            }
            else{
                StringBuffer sb=new StringBuffer(str);
                System.out.println(sb.reverse().toString());
            }
        }
    }
}
