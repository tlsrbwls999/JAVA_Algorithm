import java.util.Scanner;

public class Main {

    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        sc.nextLine();
        String num=sc.nextLine();
        int answer=0;
        
        for(int i=0;i<n;i++){
            answer+=Integer.parseInt(num.substring(i,i+1));
        }
        System.out.println(answer);
     }
}
