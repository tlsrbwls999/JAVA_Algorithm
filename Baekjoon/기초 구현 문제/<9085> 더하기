import java.util.Scanner;

public class study {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();

        for(int i=0;i<t;i++){
            int n=sc.nextInt();
            int result=0;
            for(int j=0;j<n;j++){
                result+=sc.nextInt();
            }
            System.out.println(result);
        }

    }
}
